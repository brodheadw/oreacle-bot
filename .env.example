# Required Environment Variables
export MANIFOLD_API_KEY=your_manifold_api_key_here
export MARKET_SLUG=your_market_slug_here

# Optional Translation APIs (legacy fallback when no OpenAI)
export DEEPL_API_KEY=your_deepl_api_key_here
export GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# OpenAI LLM Integration (Recommended)
export OPENAI_API_KEY=your_openai_api_key_here
export OREACLE_MODEL=gpt-4o-mini
export OREACLE_MIN_CONFIDENCE=0.75

# Optional Configuration
export OREACLE_COMMENT_ONLY=1
export OREACLE_INTERVAL=900
export OREACLE_DB=./tmp/oreacle.db
export OREACLE_LOG=INFO